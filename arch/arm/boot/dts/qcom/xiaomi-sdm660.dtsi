		compatible = "ti,tas2559";
		reg = <0x4c>;
		ti,tas2559-reset-gpio = <&tlmm 43 0>;
		ti,tas2559-irq-gpio = <&tlmm 40 0>;
		ti,tas2560-irq-gpio = <&tlmm 48 0>;
		ti,tas2559-addr = <0x4c>;
		ti,tas2560-addr = <0x4d>;
