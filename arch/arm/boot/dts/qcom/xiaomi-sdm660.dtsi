&pm660_gpios {
	/* disable pmic gpio_3 for dspg P1 */
	gpio@c200 {
		qcom,mode = <0>;
		qcom,master-en = <0>;
		status = "okay";
	};
	/* config pmic gpio_6 as sleep clk for dspg */
	gpio@c500 {
		status = "ok";
		qcom,mode = <1>;
		qcom,pull = <4>;
		qcom,vin-sel = <0>;
		qcom,src-sel = <2>;
		qcom,master-en = <1>;
		qcom,out-strength = <2>;
	};
	/* disable pmic gpio_4 as adc for backlight therm */
	gpio@c300 {
		reg = <0xc300 0x100>;
		qcom,pin-num = <4>;
		status = "ok";
		qcom,master-en = <0>;
	};
};

